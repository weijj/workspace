package com.taobao.roadrantz;

import javax.validation.constraints.Size;
import javax.validation.constraints.Pattern;


public class Student {
	public Student()
	{}	
	
	@Size(min = 4, max = 16, message = "The length of name should be between 4 and 16")
	private String name;
	
	@Pattern(regexp = "^[a-zA-Z0-9]+$", message = "invalid password")
	private String password;
	
	@Pattern(regexp="^[a-zA-Z0-9._%+-]+@[A-Z0-9.-]+\\.[A-Z]{2,6}$", message="Invalid email address.")
	private String email;
	
	public void setName(String name) {
		this.name = name;
	}
	
	public String getName() {
		return this.name;
	}
	
	public void setPassword(String password) {
		this.password = password;
	}
	
	public String getPassword() {
		return this.password;
	}
	
	public void setEmail(String email) {
		this.email = email;
	}
	
	public String getEmail() {
		return this.email;
	}
}
